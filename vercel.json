{
  "version": 2,
  "functions": {
    "api/health.js": {
      "runtime": "nodejs18.x"
    },
    "api/login.js": {
      "runtime": "nodejs18.x"
    },
    "api/files/index.js": {
      "runtime": "nodejs18.x"
    },
    "api/files/[id].js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/api/login",
      "dest": "/api/login.js"
    },
    {
      "src": "/api/files",
      "dest": "/api/files/index.js"
    },
    {
      "src": "/api/files/(.*)",
      "dest": "/api/files/[id].js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}